{
  "name": "color-util",
  "version": "0.4.2",
  "description": "Color util with color format conversions and gradients",
  "main": "dist/ColorUtil.min.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register ./test/*.spec.js",
    "test-watch": "mocha --compilers js:babel-core/register -w ./test/*.spec.js",
    "watch": "webpack-dev-server --env development",
    "prebuild": "npm run test",
    "build": "webpack -p --env production && webpack --env benchmark",
    "docs": "jsdoc2md --template readme.hbs --files src/ColorUtil.js > README.md",
    "docs-watch": "nodemon --watch src/ColorUtil.js --exec \"npm run docs\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jakoivis/color-util.git"
  },
  "keywords": [
    "color",
    "util",
    "conversion",
    "gradient"
  ],
  "author": "Jarmo Koivisto",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jakoivis/color-util/issues"
  },
  "homepage": "https://github.com/jakoivis/color-util#readme",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "benchmark": "^2.1.4",
    "chai": "^3.5.0",
    "file-loader": "^0.11.2",
    "highcharts": "^5.0.12",
    "jquery": "^3.2.1",
    "jsdoc-to-markdown": "^3.0.0",
    "lodash": "^4.17.4",
    "mocha": "^3.3.0",
    "nodemon": "^1.11.0",
    "path": "^0.12.7",
    "webpack": "^2.6.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {}
}
